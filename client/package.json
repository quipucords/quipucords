{
  "name": "quipucords-ui",
  "version": "0.0.1",
  "description": "Quipucords UI",
  "author": "Red Hat",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/quipucords/quipucords.git"
  },
  "bugs": {
    "url": "https://github.com/quipucords/quipucords/issues"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "create-react-app": "create-react-app",
    "start": "npm-run-all -lp api:mock start:client",
    "start:client": "react-scripts start",
    "start:quipucords": "npm run api:quipucords; open http://localhost:4000/index.html",
    "api:mock": "sh ./scripts/api.sh -p 4000 -f \"$(pwd)/../docs/swagger.yml\"",
    "api:quipucords": "sh ./scripts/api.sh -p 4000",
    "api:update": "npm run api:mock -- -u; npm run api:quipucords -- -u",
    "build": "react-scripts build",
    "build:prod": "npm run build && mkdir -p ../quipucords/client && cp -R ./build/* ../quipucords/client",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "patternfly": "^3.35.1",
    "patternfly-react": "^0.24.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "create-react-app": "^1.4.3"
  }
}
